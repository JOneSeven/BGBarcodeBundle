{
    "name": "bitgrave/barcode-bundle",
    "type": "symfony-bundle",
    "description": "provide barcode rendering service for your Symfony2 application.",
    "keywords": ["barcode", "datamatrix", "qrcode", "pdf417", "generator"],
    "homepage": "https://github.com/paterik/BGBarcodeBundle",
    "license": "MIT",
    "authors": [
        {
            "name":"Patrick Paechnatz",
            "email":"patrick.paechnatz@gmail.com",
            "role": "developer"
        },
        {
            "name": "BGBarcodeBundle Community",
            "homepage": "https://github.com/paterik/BGBarcodeBundle/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "bitgrave/barcode-generator": "master",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "bitgrave/barcode-generator",
                "version": "master",
                "dist": {
                    "url": "https://github.com/paterik/BGBarcodeGenerator.git/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/paterik/BGBarcodeGenerator.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "files" : [
                        "Base1DBarcode.php",
                        "Base2DBarcode.php"
                    ]
                }
            }
        }
    ],
    "autoload": {
        "psr-0": { "BG\\BarcodeBundle": "" }
    },
    "target-dir": "BG/BarcodeBundle"
}